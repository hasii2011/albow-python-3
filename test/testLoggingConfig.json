{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "simple": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        },
        "demoSimple": {
            "format": "%(levelname)s: %(module)s: %(message)s"
        }
    },
    "handlers": {
        "consoleHandler": {
            "class": "logging.StreamHandler",
            "level": "INFO",
            "formatter": "demoSimple",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "root": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ]
        },
        "__main__": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ]
        },
        "TestScheduler": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ]
        },
        "TestScheduledCall": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ]
        },
        "albow.core.Scheduler": {
            "level": "DEBUG",
            "handlers": [
                "consoleHandler"
            ]
        }


    }
}