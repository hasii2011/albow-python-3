{
    "version": 1,
    "disable_existing_loggers": "False",
    "formatters": {
        "simple": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        },
        "demoSimple": {
            "format": "%(levelname)s: %(module)s: %(message)s"
        }
    },
    "handlers": {
        "consoleHandler": {
            "class": "logging.StreamHandler",
            "formatter": "demoSimple",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "root": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "test": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "albow": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },

    }
}