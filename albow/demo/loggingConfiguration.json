{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "simple": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        },
        "demoSimple": {
            "format": "%(asctime)s.%(msecs)06d - %(levelname)s: %(module)s: %(message)s",
            "datefmt" : "%H:%M:%S"
        }
    },
    "handlers": {
        "consoleHandler": {
            "class": "logging.StreamHandler",
            "formatter": "demoSimple",
            "stream": "ext://sys.stdout",
            "level": "INFO"
        }
    },
    "loggers": {
        "root": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "0"
        },
        "__main__": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "0"
        },

        "albow.core.Widget": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "0"
        },
        "albow.widgets.Button": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "0"
        },
         "albow.widgets.ImageButton": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "0"
        },
        "albow.widgets.Label": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "0"
        },

        "albow.themes.ThemeProperty": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "0"
        },
        "albow.themes.Theme": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "0"
        },
        "albow.demo.screens.DemoMenuBarScreen": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "0"
        },
        "albow.demo.screens.DemoMenuScreen": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "0"
        },
        "albow.demo.screens.DemoUserEventsScreen": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "0"
        },

        "albow.media.EnableMusicControl": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "0"
        },
        "albow.widgets.ListBox": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "0"
        },
        "albow.core.root": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "0"
        }
    }
}