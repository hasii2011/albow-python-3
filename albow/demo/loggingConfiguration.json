{
    "version": 1,
    "disable_existing_loggers": "False",
    "formatters": {
        "simple": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        },
        "demoSimple": {
            "format": "%(asctime)s.%(msecs)06d - %(levelname)s: %(module)s: %(message)s",
            "datefmt" : "%H:%M:%S"
        }
    },
    "handlers": {
        "consoleHandler": {
            "class": "logging.StreamHandler",
            "formatter": "demoSimple",
            "stream": "ext://sys.stdout",
            "level": "INFO"
        }
    },
    "loggers": {
        "root": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "0"
        },
        "albow": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "__main__": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        }
    }
}